{
    "name": "shortener",
    "version": "3.0.0",
    "description": "The most useful API to short URL's",
    "main": "src/index.js",
    "scripts": {
        "start": "PORT=80 node --exec babel-node src",
        "dev": "nodemon --exec babel-node src",
        "prod": "pm2 start src --interpreter babel-node",
        "build:dist": "babel src --out-dir dist",
        "debug": "node inspect src",
        "deploy": "now --prod"
    },
    "author": "dannegm",
    "license": "MIT",
    "engines": {
        "node": "10.x"
    },
    "dependencies": {
        "@babel/polyfill": "^7.6.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.2",
        "cli-highlight": "^2.1.1",
        "colors": "^1.4.0",
        "cors": "^2.8.4",
        "dotenv": "^5.0.1",
        "express": "^4.17.1",
        "express-session": "^1.15.6",
        "helmet": "^3.21.2",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.22.1",
        "mongoose": "^5.0.17",
        "serve-favicon": "^2.5.0",
        "uuid": "^3.2.1",
        "winston": "^2.4.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.6.4",
        "@babel/node": "^7.6.3",
        "@babel/preset-env": "^7.6.3",
        "babel-plugin-module-resolver": "^3.2.0",
        "nodemon": "^1.17.3"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/dannegm/shortener.git"
    },
    "bugs": {
        "url": "https://github.com/dannegm/shortener/issues"
    },
    "homepage": "https://github.com/dannegm/shortener#readme"
}
